package application;

import java.util.ArrayList;
import java.util.List;

public class Lista {

	public static void main(String[] args) {

		List<String> list = new ArrayList<>();
		
		list.add("GUILHERME");
		list.add("SANTANA");
		list.add(2, "SANTOS");
		
		for(String x : list) {
			System.out.println(x);
		}
	}

}
