import java.util.Scanner;

public class Program {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		System.out.print("QUANTOS NUMEROS INTEIROS SERÃO DIGITADOS: ");
		int n = sc.nextInt();
		int soma = 0;
		
		for ( int i = n; i <= n; i++) {
			System.out.println("VALOR #" + i + ": ");
			int x = sc.nextInt();
			soma += x;
		}
		
		
		sc.close();
	}

}
