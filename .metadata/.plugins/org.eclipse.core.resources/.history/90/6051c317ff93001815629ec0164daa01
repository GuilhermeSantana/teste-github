package funcoes;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		System.out.println("DIGITE TRES NUMEROS: ");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();

		if (a > b && a > c) {
			System.out.println("O MAIOR É O PRIMEIRO NUMERO: " + a);
		} else if (b > c) {
			System.out.println("O MAIOR É O SEGUNDO NUMERO: " + b);
		} else {
			System.out.println("O MAIOR É O TERCEIRO NUMERO: " + c);
		}

		sc.close();
	}

}
